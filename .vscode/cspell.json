{
    "version": "0.2",
    "language": "en",
    "ignoreWords": [],
    "words": [],
    "languageSettings": [],
    "overrides": [
        // Windows
        {
            "filename": ["win32*.hpp", "win32*.cpp"],
            "ignoreWords": [
                "HINSTANCE",
                "HREDRAW",
                "LMEM",
                "LPCSTR",
                "lpfn",
                "lpsz",
                "LPTR",
                "OVERLAPPEDWINDOW",
                "WNDCLASSA",
                "WNDPROC",
            ]
        },
        // Batch
        {
            "filename": "*.bat",
            "ignoreWords": [
                "endlocal",
                "ERRORLEVEL",
                "popd",
                "pushd",
                "setlocal",
                "taskkill",
                "vcvarsall",
                "VSCMD",
            ]
        },
        // Generic
        {
            "filename": "*.hpp",
            "ignoreWords": []
        }
    ]
}